# ALL Linuxserver containers
PUID=1000
PGID=1000
TZ=Etc/UTC

# Gluetun
# See https://github.com/qdm12/gluetun-wiki/tree/main/setup#setup
GLUETUN_VERSION=v3.37.0
GLUETUN_PORT_HTTP_PROXY=8888
GLUETUN_PORT_SHADOWSOCKS=8388
VPN_SERVICE_PROVIDER=
VPN_TYPE=
OPENVPN_USER=
OPENVPN_PASSWORD=
FREE_ONLY=
# See https://github.com/qdm12/gluetun-wiki/blob/main/setup/servers.md#update-the-vpn-servers-list
UPDATER_PERIOD=24h

#Bazarr
BAZARR_VERSION=1.4.0
BAZARR_PORT=6767

#Jellyfin
JELLYFIN_VERSION=10.8.13
JELLYFIN_HTTP_PORT=8096
JELLYFIN_HTTPS_PORT=8920
JELLYFIN_SERVICE_DISCOVERY_PORT=1900
JELLYFIN_CLIENT_ODISCOVERY_PORT=7359
JELLYFIN_PUBLISHEDSERVERURL=192.168.0.5

#Emby
EMBY_VERSION=4.7.14
EMBY_PORT=8096
EMBY_CUSTOM_PORT=8920

#Lidarr
LIDARR_VERSION=2.0.7
LIDARR_PORT=8686

#Ombi
OMBI_VERSION=4.43.5
OMBI_PORT=3579

#Prowlarr
PROWLARR_VERSION=1.11.4
PROWLARR_PORT=9696

#Qbittorrent
QBITTORRENT_VERSION=4.6.2
QBITTORRENT_PORT=6881
QBITTORRENT_PORT_WEBI=8080

#Radaar
RADARR_VERSION=5.2.6
RADARR_PORT=7878

#Readarr
READARR_VERSION=0.3.14-nightly
READARR_PORT=8787

#Sonarr
SONARR_VERSION=4.0.0
SONARR_PORT=8989

#Filezilla
FILEZILLA_VERSION=3.66.4
FILEZILLA_HTTP_PORT=3000
FILEZILLA_HTTPS_PORT=3001
FILEZILLA_USER=
FILEZILLA_PASSWORD=
FILEZILLA_TITLE=Dionysus Client

#Heimdall
HEIMDALL_VERSION=2.5.8
HEIMDALL_PORT_HTTP=80
HEIMDALL_PORT_HTTPS=443

# Elk
# Version of Elastic products
ELK_STACK_VERSION=8.11.3

# Password for the 'elastic' user (at least 6 characters)
ELASTIC_PASSWORD=

# Port to expose Elasticsearch HTTP API to the host
ES_PORT=9200

# Password for the 'kibana_system' user (at least 6 characters)
KIBANA_PASSWORD=

# Port to expose Kibana to the host
KIBANA_PORT=5601

# Password for the 'beats_system' user (at least 6 characters)
# Unused due to to beats_system user missing roles on ilm and indexes
BEATS_PASSWORD=

# Set to 'basic' or 'trial' to automatically start the 30-day trial
LICENSE=basic
#LICENSE=trial

# Increase or decrease based on the available host memory (in bytes)
ELK_MEM_LIMIT=1GB
